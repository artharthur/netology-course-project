{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.x" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "2.x" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" }
  ],
  "annotations": {
    "list": [
      {
        "type": "dashboard",
        "builtIn": 1,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "enable": true,
        "target": { "limit": 100, "matchAny": false, "tags": [], "type": "dashboard" }
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "iteration": 1727510400000,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "timeseries",
      "title": "CPU Utilization (%)",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * sum by (instance) (rate(node_cpu_seconds_total{mode!=\"idle\",instance=~\"$instance\"}[5m])) / sum by (instance) (rate(node_cpu_seconds_total{instance=~\"$instance\"}[5m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "CPU Load / CPU",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "unit": "none" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } },
      "targets": [
        {
          "refId": "A",
          "expr": "node_load1{instance=~\"$instance\"} / machine_cpu_cores{instance=~\"$instance\"}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "RAM — Utilization (%)",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 7 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * (1 - (node_memory_MemAvailable_bytes{instance=~\"$instance\"} / node_memory_MemTotal_bytes{instance=~\"$instance\"}))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP Response Size (B/s)",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 7 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "unit": "Bps" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (instance) (rate(nginx_http_response_size_bytes{instance=~\"$instance\"}[1m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Disk — Utilization (%)",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * (1 - (sum by(instance) (node_filesystem_avail_bytes{fstype!~\"tmpfs|overlay\",mountpoint=\"/\",instance=~\"$instance\"}) / sum by(instance) (node_filesystem_size_bytes{fstype!~\"tmpfs|overlay\",mountpoint=\"/\",instance=~\"$instance\"})))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Disk — Saturation (I/O busy, %)",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by(instance) (rate(node_disk_io_time_seconds_total{instance=~\"$instance\"}[5m])) * 100"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Network — Utilization (RX/TX, B/s)",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 21 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "unit": "Bps" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "refId": "A",
          "legendFormat": "rx - {{instance}}",
          "expr": "sum by(instance) (rate(node_network_receive_bytes_total{device!~\"lo\",instance=~\"$instance\"}[1m]))"
        },
        {
          "refId": "B",
          "legendFormat": "tx - {{instance}}",
          "expr": "sum by(instance) (rate(node_network_transmit_bytes_total{device!~\"lo\",instance=~\"$instance\"}[1m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Network — Errors (errors/s)",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 21 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "refId": "A",
          "legendFormat": "net errs - {{instance}}",
          "expr": "sum by(instance) (rate(node_network_receive_errs_total{device!~\"lo\",instance=~\"$instance\"}[1m])) + sum by(instance) (rate(node_network_transmit_errs_total{device!~\"lo\",instance=~\"$instance\"}[1m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP Statuses (rps): 2xx/4xx/5xx",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 28 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "unit": "reqps" }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" },
        "stacking": { "mode": "normal", "group": "A" }
      },
      "targets": [
        {
          "refId": "A",
          "legendFormat": "2xx - {{instance}}",
          "expr": "sum by (instance) (rate(nginx_http_response_count_total{status=~\"2..\",instance=~\"$instance\"}[1m]))"
        },
        {
          "refId": "B",
          "legendFormat": "4xx - {{instance}}",
          "expr": "sum by (instance) (rate(nginx_http_response_count_total{status=~\"4..\",instance=~\"$instance\"}[1m]))"
        },
        {
          "refId": "C",
          "legendFormat": "5xx - {{instance}}",
          "expr": "sum by (instance) (rate(nginx_http_response_count_total{status=~\"5..\",instance=~\"$instance\"}[1m]))"
        }
      ]
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["web", "node", "nginx"],
  "templating": {
    "list": [
      {
        "type": "query",
        "name": "instance",
        "label": "instance",
        "hide": 0,
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "definition": "label_values(up{job=~\"node|nginx_logs|prometheus\"}, instance)",
        "query": { "query": "label_values(up{job=~\"node|nginx_logs|prometheus\"}, instance)", "refId": "PromVar" },
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "sort": 0,
        "current": {}
      }
    ]
  },
  "time": { "from": "now-15m", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "Web Monitoring",
  "uid": "web-monitoring",
  "version": 1,
  "weekStart": ""
}
